{
  "name": "@ukyijs/eslint-plugin-ukyi-config",
  "version": "1.0.0",
  "description": "JavaScript, TypeScript, React 및 포맷팅을 위한 사전 구성된 ESLint 플러그인",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "eslint-config",
    "typescript",
    "react",
    "formatting",
    "code-style"
  ],
  "author": "Ukyi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukyiJS/eslint-plugin-ukyi-config.git"
  },
  "bugs": {
    "url": "https://github.com/ukyiJS/eslint-plugin-ukyi-config/issues"
  },
  "homepage": "https://github.com/ukyiJS/eslint-plugin-ukyi-config#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "peerDependencies": {
    "@eslint/js": ">=9.0.0",
    "@stylistic/eslint-plugin": ">=2.0.0",
    "eslint": ">=9.0.0",
    "eslint-import-resolver-typescript": ">=4.4.4",
    "eslint-plugin-import": ">=2.32.0",
    "eslint-plugin-jsx-a11y": ">=6.8.0",
    "eslint-plugin-react": ">=7.33.0",
    "eslint-plugin-react-hooks": ">=5.0.0",
    "eslint-plugin-react-refresh": ">=0.4.0",
    "typescript-eslint": ">=7.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@stylistic/eslint-plugin": "^2.0.0",
    "@types/eslint": "^9.0.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "semantic-release": "^24.2.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^7.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "license": "MIT"
}
